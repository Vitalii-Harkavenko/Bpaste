generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model users {
  id       Int    @id @default(autoincrement())
  name     String @unique(map: "name") @db.VarChar(20)
  password String @db.VarChar(255)
}
